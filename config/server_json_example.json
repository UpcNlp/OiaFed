{
  "server": {
    "id": "json_fed_server",
    "trainer": {
      "type": "SimpleTrainer",
      "args": [],
      "kwargs": {
        "aggregation_method": "fedavg",
        "client_sampling_ratio": 0.8,
        "min_participants": 3,
        "max_participants": 10
      }
    },
    "transport": {
      "type": "memory",
      "kwargs": {
        "node_id": "json_fed_server"
      }
    },
    "auto_start": true
  },
  "global": {
    "experiment_name": "json_federation_demo",
    "log_level": "DEBUG",
    "save_model_path": "./outputs/models",
    "save_results_path": "./outputs/results"
  }
}
